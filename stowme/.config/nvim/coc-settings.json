{
  "diagnostic.enableSign": false,
  "coc.preferences.useQuickfixForLocations": true,
  "python.linting.flake8Enabled": true,
  "python.pythonPath": "/usr/bin/python3",
  "languageserver": {
    "ccls": {
      "command": "ccls",
