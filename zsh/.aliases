#!/usr/bin/zsh
alias e='nvim'
alias vim='nvim'
alias ls='ls --color=auto'
alias telnet='telnet -e ^C'
alias fd='fd -I --hidden'
alias rg='rg --smart-case'
alias q='qalc'
alias meteo='curl wttr.in/lausanne'
alias parrot='curl parrot.live'

alias gs='git status'
alias gsu='git status -uno'
alias ga='git add'
