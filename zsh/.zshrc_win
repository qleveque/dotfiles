cmd.exe /C rundll32.exe

export DISPLAY=:0.0
export GDK_DPI_SCALE=1.5
setxkbmap wh

for filename in ~/mybats/*; do
    leaf=$(basename $filename)
    com=${leaf%.*}
    repl="cmd.exe /C %HOME%/mybats/${com}.bat"
    alias "$com=$repl"
done
unset leaf
unset com
unset repl

cmd(){
    cmd.exe /C $@
}

unset -f open
open(){
    if [[ -n "$@" ]]
    then
        /mnt/c/Windows/System32/cmd.exe /C start "" "$@"
    fi
}

